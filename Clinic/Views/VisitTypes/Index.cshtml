@model IEnumerable<Clinic.Models.VisitType>

@{
    ViewData["Title"] = "Index";
}

<h1>Visit Types</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VisitTypeID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VisitTypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VisitTypeID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VisitTypeName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.VisitTypeID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.VisitTypeID })
            </td>
        </tr>
}
    </tbody>
</table>
